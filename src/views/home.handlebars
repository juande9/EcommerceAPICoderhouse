<div>
    <h1>Productos sin websocket</h1>
    <table>
        <tr>
            <th>Producto</th>
            <th>Description</th>
            <th>Price</th>
            <th>Code</th>
            <th>Stock</th>
            <th>Id</th>
        </tr>
        {{#each data}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.description}}</td>
            <td>{{this.price}}</td>
            <td>{{this.code}}</td>
            <td>{{this.stock}}</td>
            <td>{{this.id}}</td>
        </tr>
        {{/each}}
    </table>
</div>
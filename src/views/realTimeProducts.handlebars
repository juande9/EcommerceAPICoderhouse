<div>
    <div>
        <h1>Agregar producto</h1>
        <form id="form">
            <label for="product">Producto:</label>
            <input type="text" id="product" required><br><br>

            <label for="description">Descripción:</label>
            <input type="text" id="description" required><br><br>

            <label for="price">Precio:</label>
            <input type="number" id="price" required><br><br>

            <label for="code">Código:</label>
            <input type="text" id="code" required><br><br>

            <label for="stock">Stock:</label>
            <input type="number" id="stock" required><br><br>

            <label for="thumbnail">Thumbnail:</label>
            <input type="file" id="thumbnail" accept="image/*"><br><br>

            <button type="submit">Agregar producto</button>
        </form>
    </div>
    <h1>Productos actualizados en tiempo real</h1>
    <table id="productlist">
        <tr>
            <th>Producto</th>
            <th>Description</th>
            <th>Price</th>
            <th>Code</th>
            <th>Stock</th>
            <th>Id</th>
        </tr>
        {{#each data}}
        <tr>
            <td>{{this.title}}</td>
            <td>{{this.description}}</td>
            <td>{{this.price}}</td>
            <td>{{this.code}}</td>
            <td>{{this.stock}}</td>
            <td>{{this.id}}</td>
        </tr>
        {{/each}}
    </table>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/4.6.1/socket.io.js"></script>
<script src="/js/realTimeProduct.js"></script>